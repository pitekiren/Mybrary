<h2>Find Book</h2>
<form action="/books" method="GET">
  <div>
    <label>Title</label>
    <input type="text" name="title" value="<%=searchOption.title%>" />
  </div>

  <div>
    <label>Published After</label>
    <input
      type="date"
      name="publishedAfter"
      value="<%=searchOption.publishedAfter%>"
    />
  </div>
  <div>
    <label>Published Before</label>
    <input
      type="date"
      name="publishedBefore"
      value="<%=searchOption.publishedBefore%>"
    />
  </div>

  <button type="submit">Search</button>
</form>
<br />
<div>
  <%books.forEach(book => {%>
  <img src="<%=book.coverImagePath%>" alt="" height="150" width="100" />
  <%})%>
</div>
